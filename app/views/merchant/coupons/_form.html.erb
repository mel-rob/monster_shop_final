
<%= form_for([current_user.merchant, @coupon]) do |form| %>

  <%= form.label :name %>
  <%= form.text_field :name%>

  <%= form.label :percentage_off %>
  <%= form.number_field :percentage_off, min: 0, max: 100 %>

  <%= form.label :code %>
  <%= form.number_field :code %>

  <%= form.submit 'Submit' %>

<% end %>
